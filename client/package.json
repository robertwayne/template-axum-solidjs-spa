    "happy-dom": "^13.0.4",
    "postcss": "^8.4.33",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.2.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "solid-js": "^1.8.11",
    "tailwindcss": "^3.4.1",
    "terser": "^5.26.0",
