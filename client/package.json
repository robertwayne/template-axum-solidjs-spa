        "postcss": "^8.4.21",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.8.7",
        "prettier-plugin-tailwindcss": "^0.2.6",
        "solid-js": "^1.6.16",
        "tailwindcss": "^3.3.0",
        "typescript": "^5.0.2",
