        "happy-dom": "^13.3.5",
        "postcss": "^8.4.33",
        "postcss-load-config": "^5.0.2",
        "prettier": "^3.2.4",
        "prettier-plugin-tailwindcss": "^0.5.11",
        "solid-js": "^1.8.12",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3",
