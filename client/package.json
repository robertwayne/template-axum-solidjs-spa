        "postcss": "^8.4.32",
        "postcss-load-config": "^5.0.2",
        "prettier": "^3.1.1",
        "prettier-plugin-tailwindcss": "^0.5.9",
        "solid-js": "^1.8.7",
        "tailwindcss": "^3.4.0",
