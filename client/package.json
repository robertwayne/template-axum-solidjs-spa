        "postcss": "^8.4.21",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.8.6",
        "prettier-plugin-tailwindcss": "^0.2.5",
        "solid-js": "^1.6.15",
        "tailwindcss": "^3.2.7",
        "typescript": "^5.0.2",
