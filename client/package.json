{
    "name": "template-axum-solidjs-spa",
    "version": "0.1.0",
    "repository": "https://github.com/robertwayne/template-axum-solidjs-spa",
    "author": "Rob Wagner <rob@sombia.com>",
    "license": "MIT-0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "fmt": "prettier --write  \"**/*.{ts,js,tsx,jsx}\"",
        "fmt:check": "prettier --check  \"**/*.{ts,js,tsx,jsx}\"",
        "tsc": "tsc --noEmit",
        "tsc:watch": "tsc --noEmit --watch"
    },
    "dependencies": {
        "@solidjs/router": "0.15.3",
        "solid-js": "1.9.6"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.1.5",
        "@types/bun": "~1.2.12",
        "prettier": "~3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "tailwindcss": "^4.1.5",
        "typescript": "~5.8.3",
        "vite": "^6.3.5",
        "vite-plugin-solid": "2.11.6"
    },
    "prettier": {
        "tabWidth": 4,
        "semi": false,
        "tailwindStylesheet": "./src/index.css",
        "plugins": [
            "prettier-plugin-tailwindcss"
        ]
    }
}