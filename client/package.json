        "happy-dom": "^12.1.5",
        "postcss": "^8.4.30",
        "postcss-load-config": "^4.0.1",
        "prettier": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.4",
        "solid-js": "^1.7.12",
        "tailwindcss": "^3.3.3",
        "terser": "^5.20.0",
